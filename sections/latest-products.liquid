     {% for block in section.blocks %} 
    <div class="item">
     {% assign b = all_products[block.settings.featured_product] %} 
      <div class="el-wrapper">
        <div class="box-up">
          <a href="{{ b.url }}">
          <img class="img" src="{{ b.featured_image | img_url: 'large' }}" alt="">
          </a>
          <div class="img-info">
            <div class="info-inner">
              <span class="p-name">{{ b.title }}</span>
              <span class="p-company">EK's New Life</span>
            </div>
            <!--<div class="a-size">Description : <span class="size">S , M , L , XL</span></div>-->
          </div>
        </div>

        <div class="box-down">
          <div class="h-bg">
            <div class="h-bg-inner"></div>
          </div>

          <a class="cart" href="https://eks-new-life.myshopify.com/cart/add?id={{ b.variants.first.id }}&quantity=1">
            <span class="price">{{b.price | money}} </span>
            <span class="add-to-cart">
              <span class="txt">{{ block.settings.button_label }}</span>
            </span>
          </a>
        </div>
      </div>
      </div>
      {% endfor %}        
      </div> 
      <div class="col-12 d-flex align-items-center justify-content-center">
        <a href="{{ section.settings.button_link }}" class="scanfcode">
          {{ section.settings.button_label }}
        </a>
      </div>
      
    </div>
  </div>
