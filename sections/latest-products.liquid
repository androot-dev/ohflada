{% schema %}
  {
    "name": "Latest Products",
    "settings": [

       {
		"type": "text",
        "id": "title",
		"label": "Title Section",
        "default": "Products"
        
     },
     {
		"type": "text",
        "id": "subtitle",
		"label": "SubTitle Section",
        "default": "Subtitle"
      },
     {
      "type": "text",
      "id": "button_label",
      "label": "Button Label",
	  "default": "View More Products"
      },
      {
      "type": "url",
      "id": "button_link",
      "label": "Button Link"
      }

     ],
     "blocks": [
      {
       "type": "product",
        "name": "Product", 
     "settings": [
    {
      "type": "product",
      "id": "featured_product",
      "label": "Product"      
    },
      {
      "type": "text",
      "id": "button_label",
      "label": "Button Label",
      "default": "Add To Cart"
    }
	]
	}
    ],   
  	"presets": [
      {
        "category": "Home",
        "name": "Latest Products"     
        
      }
    ]
  }
{% endschema %}

