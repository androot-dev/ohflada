<style>  
  .about-us{
    background: var(--primary-light);
    padding-top: 50px;
    padding-bottom: 40px;
    position: relative;
    overflow: hidden;
  }
  

  
  
.about-us .subtitle {
 
   margin-bottom: 20px;   
}

@media (max-width: 575px){
 
  
  .about-us .subtitle {
   text-align: center;   
}
}
  
[data-aos].btn{
  transition-property: all !important;
}
  
 
.about-us p {
  position: relative;
  z-index: 2;  
}
  

@keyframes inclinarYregresar {
  0% {
    transform: rotate(0deg);
  }
  50% {
    transform: rotate(3deg);
  }
  100% {
    transform: rotate(0deg);
  }
}


.btn[data-aos]{
  transition: 
}
  
.about-us .img-1{
  width: 450px;
  height: 450px;
  z-index: 0;
  position: relative;
  /* border: 4px solid #fff; */
  box-shadow: rgb(255 192 192 / 38%) 5px 5px, rgb(224 61 61 / 30%) 10px 10px, rgb(255 0 0 / 20%) 15px 15px;
  border-radius: var(--radius);

  object-fit: cover;
   /*animation: inclinarYregresar 2.5s infinite; */
}

.about-us .about-us-square:after{
  
  position: absolute;
  top: -10%;
  left: -3%;
  width: 500px;
  height: 500px; 
  content: '';
  background-image: linear-gradient(to left bottom, #858383, #999797, #aeacac, #c3c1c1, #d9d7d7);
  z-index: 1;
  border-radius: 10px;
}
  

  
  @media only screen and (max-width: 767px) {
.about-us .img-1{
   box-shadow:var(--shadow) !important; 
}
    .about-us .img-1 {
    height: 365px;

}
    
 .about-us p {
text-align: center;
}
  
  }



