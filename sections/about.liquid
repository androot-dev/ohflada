<section class="about-us">
<style>
  .gallery-home{
   padding: 7em 0;
   overflow: hidden;
   position: relative;   
   background: #ffe4e1;   
  }
  
/* SVG TOP*/
  .custom-shape-divider-top-1628448272 {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    overflow: hidden;
    line-height: 0;
}

.custom-shape-divider-top-1628448272 svg {
    position: relative;
    display: block;
    width: calc(164% + 1.3px);
    height: 103px;
}

.custom-shape-divider-top-1628448272 .shape-fill {
    fill: #FFFFFF;
}
  
  
/*SVG BOTTOM*/
  
.custom-shape-divider-bottom-1628450483 {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    overflow: hidden;
    line-height: 0;
    transform: rotate(180deg);
}

.custom-shape-divider-bottom-1628450483 svg {
    position: relative;
    display: block;
    width: calc(164% + 1.3px);
    height: 103px;
}

.custom-shape-divider-bottom-1628450483 .shape-fill {
    fill: #FFFFFF;
}
  
  
  
.gallery-home img{
 -webkit-transition:all .6s ease-out 0s;
 -moz-transition:all .6s ease-out 0s;
 -ms-transition:all .6s ease-out 0s;
 -o-transition:all .6s ease-out 0s;
 transition:all .6s ease-out 0s;
 position:relative;
 top:5px;
 margin-bottom:25px;
 width: 60%
}
	
@media (max-width: 575px){
.gallery-homeimg{
    width:95%
  }
}

.gallery-home h2{
   color:#383838;
   font-size:32px;
   font-weight:300;
   line-height:1.3;   
  letter-spacing: 0 !important;
  text-transform: none !important;
  font-family: "Fuente-maquina-italica" !important;
}

@media (max-width:768px){
.gallery-home  h2{
    font-size:24px;
    line-height:1.3
  }
  
  .custom-shape-divider-top-1628448272 svg {    
    height: 40px;
}
.custom-shape-divider-bottom-1628450483 svg {    
    height: 40px;
}
}
  

</style>
  
